            {
                "type": "copyAndReplace",
                "srcFileUrl": "/apogeejs-app-lib/package.json",
                "replacementList": [
                    {
                        "marker": "[RELEASE_VERSION]",
                        "lib": "this",
                        "type": "version"
                    },
                    {
