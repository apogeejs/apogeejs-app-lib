                        "lib": "apogeejs-util-lib",
                        "type": "npmVersion"
                    },
                    {
                        "marker": "[APOGEE_BASE_DEP_VERSION]",
                        "lib": "apogeejs-base-lib",
                        "type": "npmVersion"
                    },
                    {
                        "marker": "[APOGEE_MODEL_DEP_VERSION]",
                        "lib": "apogeejs-model-lib",
                        "type": "npmVersion"
                    }
                ]
            },
            {
                "type": "packageLib",
                "esModuleFileName": "apogeeAppLib.js",
                "npmModuleFileName": "apogeejs-app-lib.js",
                "externalLibs": [
                    "/apogeejs-base-lib/src/apogeeBaseLib.js",
                    "/apogeejs-util-lib/src/apogeeUtilLib.js",
                    "/apogeejs-model-lib/src/apogeeModelLib.js",
                    "/prosemirror-admin/dist/prosemirror-model.es.js",
                    "/prosemirror-admin/dist/prosemirror-state.es.js",
                    "/prosemirror-admin/dist/prosemirror-transform.es.js",
                    "/prosemirror-admin/dist/prosemirror-gapcursor.es.js"
                ],
                "externalLibMapping": {
                    "/apogeejs-base-lib/src/apogeeBaseLib.js": "apogeejs-base-lib",
                    "/apogeejs-util-lib/src/apogeeUtilLib.js": "apogeejs-util-lib",
