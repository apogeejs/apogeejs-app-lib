                    "/prosemirror-admin/dist/prosemirror-model.es.js": "prosemirror-model",
                    "/prosemirror-admin/dist/prosemirror-state.es.js": "prosemirror-state",
                    "/prosemirror-admin/dist/prosemirror-transform.es.js": "prosemirror-transform",
                    "/prosemirror-admin/dist/prosemirror-gapcursor.es.js": "prosemirror-gapcursor"
                }
            }
        ]
    }
}